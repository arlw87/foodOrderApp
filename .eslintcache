tConfig.url, {\n                method: requestConfig.method ? requestConfig.method : 'GET',\n                headers: requestConfig.headers ? requestConfig.headers : {},\n                body: requestConfig.body ? JSON.stringify(requestConfig.body) : null,\n            });\n\n            if (!response.ok){\n                throw new Error('Request Failed')\n            }\n\n            const json = await response.json();\n            applyData(json);\n\n        } catch (error){\n            setError(error.message)\n        } \n        \n        setIsLoading(false);\n    },[])\n\n    return [isLoading, error, sendRequest];\n\n}\n\nexport default useFetch;\n","/Users/andrew/Programming/React/foodOrderApp/src/components/order/CheckOut.js",[],"/Users/andrew/Programming/React/foodOrderApp/src/components/hooks/useInput.js",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":14,"column":17,"nodeType":"102","endLine":14,"endColumn":38},{"ruleId":"103","severity":1,"message":"104","line":4,"column":8,"nodeType":"105","messageId":"106","endLine":4,"endColumn":14},{"ruleId":"103","severity":1,"message":"107","line":3,"column":8,"nodeType":"105","messageId":"106","endLine":3,"endColumn":16},{"ruleId":"108","severity":1,"message":"109","line":46,"column":7,"nodeType":"110","endLine":46,"endColumn":20,"suggestions":"111"},{"ruleId":"103","severity":1,"message":"112","line":1,"column":8,"nodeType":"105","messageId":"106","endLine":1,"endColumn":23},{"ruleId":"103","severity":1,"message":"113","line":1,"column":10,"nodeType":"105","messageId":"106","endLine":1,"endColumn":19},"no-native-reassign",["114"],"no-negated-in-lhs",["115"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-unused-vars","'Styles' is defined but never used.","Identifier","unusedVar","'MealForm' is defined but never used.","react-hooks/ex